{
  "buildCommand": null,
  "outputDirectory": "dist",
  "framework": null,
  "redirects": [
    { "source": "/", "destination": "https://docs.glyph.you/sdk/installation", "permanent": false }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
