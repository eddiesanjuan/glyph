{
  "name": "@glyphpdf/sdk",
  "version": "0.7.0",
  "description": "AI PDF generation web component - Embed natural language PDF editing in any app",
  "type": "module",
  "main": "./dist/glyph.cjs.js",
  "module": "./dist/glyph.esm.js",
  "browser": "./dist/glyph.esm.js",
  "unpkg": "./dist/glyph.min.js",
  "jsdelivr": "./dist/glyph.min.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/glyph.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/glyph.cjs.js"
      }
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test:serve": "node test/serve.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai pdf generation",
    "pdf api",
    "document generation",
    "natural language pdf",
    "pdf templates",
    "mcp server",
    "anthropic",
    "claude",
    "web-component",
    "embeddable",
    "sdk"
  ],
  "author": {
    "name": "Eddie San Juan",
    "email": "eddie@glyphpdf.com"
  },
  "license": "BUSL-1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eddiesanjuan/glyph.git",
    "directory": "sdk"
  },
  "homepage": "https://glyphpdf.com",
  "bugs": {
    "url": "https://github.com/eddiesanjuan/glyph/issues"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
